create table Tweets (
tweet_id int primary key, content varchar(30)
);
insert into Tweets (tweet_id, content) values (1, Vote for Biden);
insert into Tweets (tweet_id, content) values (2, Let us make America great again!);
select tweet_id 
from Tweets
where char_length(content) > 15;
/*Here a special function is used to pick the tweet_id whose content column 
related has a 'string' data type more than 15 characters.*/
